{
  "name": "@slickquant/slick-ladder",
  "version": "0.1.1",
  "description": "Ultra-low latency price ladder component for web",
  "main": "dist/slick-ladder.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/slick-ladder.js"
    },
    "./main": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./wasm-adapter": {
      "types": "./dist/wasm-adapter.d.ts",
      "default": "./dist/wasm-adapter.js"
    },
    "./canvas-renderer": {
      "types": "./dist/canvas-renderer.d.ts",
      "default": "./dist/canvas-renderer.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./wasm-types": {
      "types": "./dist/wasm-types.d.ts",
      "default": "./dist/wasm-types.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "prepublishOnly": "cd ../.. && npm run build:lib:full",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlickQuant/slick-ladder",
    "directory": "src/web"
  },
  "bugs": {
    "url": "https://github.com/SlickQuant/slick-ladder/issues"
  },
  "homepage": "https://github.com/SlickQuant/slick-ladder#readme",
  "keywords": [
    "price-ladder",
    "trading",
    "orderbook",
    "webassembly",
    "canvas",
    "low-latency",
    "real-time",
    "market-data",
    "dom",
    "depth-of-market"
  ],
  "author": "Slick Quant LLC.",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
